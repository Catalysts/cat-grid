{
  "name": "cat-grid-example",
  "version": "1.0.0",
  "description": "Small example for grid library",
  "scripts": {
    "copyLib": "cpx ../src/**/* ./src/app/lib/src/",
    "build": "npm run copyLib && tsc -p src/",
    "build:watch": "tsc -p src/ -w",
    "build:e2e": "tsc -p ../e2e/",
    "serve": "lite-server -c=bs-config.json",
    "serve:e2e": "lite-server -c=bs-config.json",
    "prestart": "npm run build",
    "start": "concurrently \"npm run build:watch\" \"npm run serve\"",
    "pree2e": "npm run build:e2e",
    "e2e": "npm build && concurrently \"npm run serve:e2e\" \"npm run protractor\" --kill-others --success first",
    "preprotractor": "webdriver-manager update",
    "protractor": "protractor protractor.conf.js",
    "pretest": "npm run build",
    "test": "concurrently \"npm run build:watch\" \"karma start karma.conf.js\"",
    "pretest:once": "npm run build",
    "test:once": "karma start karma.conf.js --single-run",
    "lint": "tslint ./src/**/*.ts -t verbose"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@angular/common": "~8.1.1",
    "@angular/compiler": "^8.1.1",
    "@angular/core": "~8.1.1",
    "@angular/forms": "~8.1.1",
    "@angular/http": "^7.2.15",
    "@angular/platform-browser": "~8.1.1",
    "@angular/platform-browser-dynamic": "~8.1.1",
    "@angular/router": "~8.1.1",
    "core-js": "^2.4.1",
    "ng2-dragula": "^1.5.0",
    "rxjs": "^6.5.2",
    "systemjs": "0.19.40",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@types/jasmine": "3.3.15",
    "@types/node": "^12.6.8",
    "canonical-path": "0.0.2",
    "concurrently": "^3.2.0",
    "cpx": "^1.5.0",
    "jasmine-core": "~2.4.1",
    "karma": "^6.3.16",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lite-server": "^2.2.2",
    "lodash": "^4.16.4",
    "protractor": "~5.4.2",
    "rimraf": "^2.5.4",
    "tslint": "5.18.0",
    "typescript": "3.4.5"
  },
  "repository": {}
}
